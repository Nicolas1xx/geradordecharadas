{
    "version": 2,
    "builds": [
      {
        "src": "app.py",
        "use": "@vercel/python"
      }
    ],
    "routes": [
      {
        "src": "/",
        "dest": "/app.py"
      },
      {
        "src": "/nova_charada",
        "dest": "/app.py"
      },
      {
        "src": "/(.*)",
        "dest": "/app.py"
      }
    ]
  }
  